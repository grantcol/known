<%= form_for(@link) do |f| %>

	<% @link.errors.full_messages.each do |error| %>

		<%= error %>

	<% end %>

    <div>
    	<%= f.text_field :title, placeholder: 'title' %>
    </div>
    <div>
    	<%= f.text_field :url, placeholder: 'url' %>
    </div>
    <div>
        <%= f.text_field :tag_list, placeholder: 'tags' %>
    </div>
    <div>
        <%= f.text_field :channel_ids, placeholder: 'to what channels?' %>
    </div>    
    <%= f.submit %>

<% end %>
