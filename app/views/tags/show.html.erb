<tr>
	<table>
		<tr>
			<td><h2><%= @tag.name %></h2></td>
		</tr>
	</table>
</tr>
 <% @tag.links.each_with_index do |link, i| %>
  <tr>
	<td>
		<%= i + 1 %>.	 
	</td>
	<td>
    	<a href=<%link.url%> class= "link_title_tag link_title" style="text-decoration: none;"> <%= link.title %> </a>
    	<% 
		     	url = link.url
		        stripped = url.slice!("https://")
		        if stripped != "https://"
		           stripped = url.slice!("http://")
		        end
		        temp = url.split("/");
		        stripped = temp[0];
	 	%>
	 	(<%= stripped %>)
	</td>
  </tr>
<% end %>
